' pedropt10: Throttle deactivates bremse_halte

{macro:DoorAftCalc}
	(L.L.bremse_halte_sw) !
	(L.L.door_2) 0 = &&
	(L.L.door_3) 0 = &&
	(L.L.throttle) 0 > &&
	(L.L.elec_busbar_main) &&
